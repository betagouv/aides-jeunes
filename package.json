{
  "name": "mes-aides-jeunes",
  "version": "12.1.3",
  "description": "Simulateur de prestations sociales pour les jeunes",
  "engines": {
    "node": ">= 16"
  },
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/betagouv/aides-jeunes"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --no-clean",
    "test:unit": "vue-cli-service test:unit",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "build:analyze": "npm run build -- --env.BUNDLEANALYZE",
    "build:dev": "webpack --mode=development",
    "build:netlify": "npm run build && cp netlify_redirects dist/_redirects",
    "cypress": "cypress run --config-file false",
    "cypress:open": "cypress open --config-file false",
    "db": "mongod --dbpath db",
    "db-update": "./import-tests.sh",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "fast-install": "PUPPETEER_SKIP_DOWNLOAD=1 npm ci",
    "front": "NODE_ENV=front-only npm run serve",
    "install-openfisca": "pip install --upgrade -r openfisca/requirements.txt",
    "openfisca": "OPENFISCA_WORKERS=1 gunicorn openfisca.api --config openfisca/config.py",
    "predb": "mkdir -p db",
    "prestart": "npm run build && npm run stats",
    "start": "NODE_ENV=production node server.js",
    "ci": "npm run stats && NODE_ENV=production node server.js",
    "stats": "node backend/lib/stats",
    "serve-mail": "node tools/mjml.js",
    "test": "NODE_PATH=. jest --testTimeout=20000",
    "test-benefits-urls": "node tools/test-benefits-urls.js",
    "generate-missing-institutions-aides-velo": "NODE_PATH=. node tools/generate-missing-institutions-aides-velo.js",
    "test-definition-periods": "node tools/test-definition-periods.js",
    "husky": "husky install",
    "evaluate-benefits": "node tools/evaluate-benefits"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.0",
    "@etalab/decoupage-administratif": "^1.0.0",
    "@sentry/integrations": "^6.14.0",
    "@sentry/node": "^6.14.0",
    "@sentry/vue": "^6.14.0",
    "@vue/cli": "^5.0.4",
    "aides-velo": "2.0.28",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "communes-lonlat": "^1.1.0",
    "consolidate": "^0.16.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.19.1",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "currencyformatter.js": "^2.2.0",
    "errorhandler": "^1.5.1",
    "event-stream": "4.0.1",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "font-awesome": "^4.7.0",
    "haversine": "^1.1.1",
    "iframe-resizer": "^4.3.2",
    "jamstack-loader": "^0.0.9",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mjml": "^4.10.4",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.2",
    "mongodb": "^4.1.4",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "puppeteer": "11.0.0",
    "require-from-string": "^2.0.2",
    "sass": "^1.47.0",
    "sass-loader": "^12.4.0",
    "sib-api-v3-sdk": "^8.2.1",
    "template.data.gouv.fr": "^1.3.2",
    "tmp": "^0.2.1",
    "validator": "^13.7.0",
    "vue": "^3.2.32",
    "vue-matomo": "^4.1.0",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2",
    "webpack-dev-server": "^4.7.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "@vue/cli-plugin-babel": "5.0.0-beta.7",
    "@vue/cli-plugin-eslint": "5.0.0-beta.7",
    "@vue/cli-plugin-unit-jest": "5.0.0-beta.7",
    "@vue/cli-service": "5.0.0-beta.7",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "argparse": "^2.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.3.1",
    "cypress": "^9.2.1",
    "eslint": ">=7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-vue": "^8.0.3",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "memory-fs": "^0.5.0",
    "minimist": "^1.2.6",
    "mustache": "^4.2.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "webpack": "^5.64.3",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "babel": {
    "presets": [
      [
        "@vue/cli-plugin-babel/preset",
        {
          "useBuiltIns": "entry"
        }
      ]
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "strict",
    "semi": false
  },
  "browserslist": [
    "> 0.1%, last 2 versions, Firefox ESR, not dead"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "@vue/vue3-jest",
      ".+\\.(css|styl|less|scss|sass|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
