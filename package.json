{
  "name": "mes-aides-jeunes",
  "version": "12.1.3",
  "description": "Simulateur de prestations sociales pour les jeunes",
  "scripts": {
    "serve": "nodemon --exec 'babel-node -x \".ts\"' backend/dev",
    "build": "concurrently 'npm run build:server' 'npm run build:lib' 'npm run build:data' && concurrently 'npm run build:iframes' 'npm run build:front' && npm run copy:templates",
    "clean:dist-server": "rm -fR dist-server",
    "copy:templates": "cp -R backend/lib/mes-aides/emails/templates dist-server/backend/lib/mes-aides/emails/templates",
    "build:iframes": "webpack --config iframes/iframes.config.ts",
    "build:front": "vite build",
    "build:server": "babel ./backend --out-dir dist-server/backend --extensions '.ts,.js'",
    "build:lib": "babel ./lib --out-dir dist-server/lib --extensions '.ts,.js'",
    "build:data": "babel ./data --out-dir dist-server/data --extensions '.ts,.js'",
    "preview": "vite preview",
    "lint": "eslint backend data lib src ./*.ts",
    "ci": "npm run stats && cd dist-server && NODE_ENV=production node ./backend/server.js",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "db": "mongod --dbpath db",
    "db-update": "./import-tests.sh",
    "front": "NODE_ENV=front-only npm run serve",
    "husky": "husky install",
    "install-openfisca": "pip install --upgrade -r openfisca/requirements.txt",
    "lint:fix": "eslint --fix backend data lib src ./*.ts",
    "openfisca": "OPENFISCA_WORKERS=1 gunicorn openfisca.api --config openfisca/config.py",
    "predb": "mkdir -p db",
    "prestart": "npm run build && npm run stats",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "cd dist-server && NODE_ENV=production node ./backend/server.js",
    "stats": "node ./dist-server/backend/lib/stats",
    "test": "NODE_PATH=. jest --testTimeout=20000",
    "tools:check-links-validity": "node dist-server/tools/check-links-validity.js  --ci",
    "tools:cleaner": "node dist-server/tools/cleaner.js",
    "tools:evaluate-benefits": "node dist-server/tools/evaluate-benefits",
    "tools:generate-missing-institutions-aides-velo": "NODE_PATH=. node --experimental-json-modules dist-server/tools/generate-missing-institutions-aides-velo.js",
    "tools:geographical-benefits-details": "node --experimental-json-modules dist-server/tools/geographical-benefits-details.js",
    "tools:get-all-steps": "node dist-server/tools/get-all-steps.js",
    "tools:test-benefits-geographical-constraint-consistency": "node dist-server/tools/test-benefits-geographical-constraint-consistency.js",
    "tools:serve-mail": "node dist-server/tools/mjml.js",
    "tools:test-definition-periods": "node dist-server/tools/test-definition-periods.js",
    "migrate": "node dist-server/backend/lib/migrations/apply.js --all"
  },
  "dependencies": {
    "@actions/core": "^1.8.2",
    "@actions/github": "^5.0.3",
    "@etalab/decoupage-administratif": "^2.1.0",
    "@sentry/integrations": "^6.14.0",
    "@sentry/node": "^6.14.0",
    "@sentry/vue": "^6.14.0",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "aides-velo": "3.0.1",
    "axios": "^0.27.2",
    "communes-lonlat": "^1.1.0",
    "consolidate": "^0.16.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.19.1",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.3",
    "errorhandler": "^1.5.1",
    "event-stream": "4.0.1",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "haversine": "^1.1.1",
    "iframe-resizer": "^4.3.2",
    "jamstack-loader": "^0.0.9",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.range": "^3.2.0",
    "mjml": "^4.10.4",
    "mongodb": "^4.1.4",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "pinia": "^2.0.16",
    "sib-api-v3-sdk": "^8.2.1",
    "template.data.gouv.fr": "^1.3.2",
    "tmp": "^0.2.1",
    "validator": "^13.7.0",
    "vite": "^3.0.4",
    "vite-plugin-html": "^3.2.0",
    "vue": "^3.2.32",
    "vue-matomo": "^4.1.0",
    "vue-router": "^4.0.14",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.7.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.1",
    "@babel/node": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bluebird": "^3.5.36",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.14",
    "@types/iframe-resizer": "^3.5.9",
    "@types/lodash": "^4.14.182",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/vue3-jest": "^27.0.0",
    "argparse": "^2.0.1",
    "concurrently": "^7.4.0",
    "cypress": "^10.8.0",
    "eslint": ">=7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "rollup-plugin-visualizer": "^5.8.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.8.3",
    "webpack": "^5.64.3",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "strict",
    "semi": false
  },
  "jest": {
    "roots": [
      "<rootDir>/tests"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "vue"
    ],
    "transform": {
      "\\.[jt]sx?$": "ts-jest",
      "\\.vue$": "@vue/vue3-jest"
    },
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    },
    "transformIgnorePatterns": [
      "/\\./"
    ],
    "moduleNameMapper": {
      "@lib/(.*)": "<rootDir>/lib/$1",
      "@/(.*)": "<rootDir>/src/$1",
      "(.+)\\.js": "$1",
      "@root/(.*)": "<rootDir>/$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "testMatch": [
      "**/tests/(integration|unit)/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "preset": "ts-jest",
    "automock": false,
    "setupFiles": [
      "./tests/unit/mocks/config.js"
    ]
  },
  "engines": {
    "node": ">= 16"
  },
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/betagouv/aides-jeunes"
  }
}
