{
  "name": "mes-aides-jeunes",
  "version": "12.1.3",
  "description": "Simulateur de prestations sociales pour les jeunes",
  "type": "module",
  "scripts": {
    "serve": "npm run build:server && npm run build:iframes && vue-cli-service serve",
    "serve:back": "npm run build:server && node ./dist-server/server.js",
    "build": "npm run build:server && npm run build:iframes && npm run build:front",
    "clean:dist-server": "rm -fR dist-server",
    "build:analyze": "npm run build -- --env.BUNDLEANALYZE",
    "build:dev": "webpack --mode=development",
    "build:netlify": "npm run build && cp netlify_redirects dist/_redirects",
    "build:iframes": "webpack --config iframes.config.js",
    "build:front": "vue-cli-service build --no-clean",
    "build:server": "tsc -p tsconfig.server.json && cp -r lib backend data tools dist-server/",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint backend data lib src ./*.ts",
    "ci": "npm run stats && cd dist-server && NODE_ENV=production node ./server.js",
    "cypress": "cypress run --config-file false",
    "cypress:open": "cypress open --config-file false",
    "db": "mongod --dbpath db",
    "db-update": "./import-tests.sh",
    "front": "npm run build:server && NODE_ENV=front-only npm run serve",
    "husky": "husky install",
    "install-openfisca": "pip install --upgrade -r openfisca/requirements.txt",
    "lint:fix": "eslint --fix backend data lib src ./*.ts",
    "openfisca": "OPENFISCA_WORKERS=1 gunicorn openfisca.api --config openfisca/config.py",
    "predb": "mkdir -p db",
    "prestart": "npm run build && npm run stats",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "cd dist-server && NODE_ENV=production node ./server.js",
    "stats": "node ./dist-server/backend/lib/stats",
    "test": "NODE_PATH=. jest --testTimeout=20000",
    "tools:check-links-validity": "node dist-server/tools/check-links-validity.js  --ci",
    "tools:cleaner": "node dist-server/tools/cleaner.js",
    "tools:evaluate-benefits": "node dist-server/tools/evaluate-benefits",
    "tools:generate-missing-institutions-aides-velo": "NODE_PATH=. node --experimental-json-modules dist-server/tools/generate-missing-institutions-aides-velo.js",
    "tools:geographical-benefits-details": "node --experimental-json-modules dist-server/tools/geographical-benefits-details.js",
    "tools:get-all-steps": "node dist-server/tools/get-all-steps.js",
    "tools:test-benefits-geographical-constraint-consistency": "node dist-server/tools/test-benefits-geographical-constraint-consistency.js",
    "tools:serve-mail": "node dist-server/tools/mjml.js",
    "tools:test-definition-periods": "node dist-server/tools/test-definition-periods.js",
    "migrate": "node dist-server/backend/lib/migrations/apply.js --all"
  },
  "dependencies": {
    "@actions/core": "^1.8.2",
    "@actions/github": "^5.0.3",
    "@babel/runtime": "^7.16.0",
    "@etalab/decoupage-administratif": "^1.1.1",
    "@sentry/integrations": "^6.14.0",
    "@sentry/node": "^6.14.0",
    "@sentry/vue": "^6.14.0",
    "@vue/cli": "^5.0.4",
    "aides-velo": "^2.0.44",
    "axios": "^0.24.0",
    "communes-lonlat": "^1.1.0",
    "consolidate": "^0.16.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.19.1",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "currencyformatter.js": "^2.2.0",
    "dayjs": "^1.11.3",
    "errorhandler": "^1.5.1",
    "event-stream": "4.0.1",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "font-awesome": "^4.7.0",
    "haversine": "^1.1.1",
    "iframe-resizer": "^4.3.2",
    "jamstack-loader": "^0.0.9",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mjml": "^4.10.4",
    "mkdirp": "^1.0.4",
    "mongodb": "^4.1.4",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "pinia": "^2.0.16",
    "require-from-string": "^2.0.2",
    "sass": "^1.47.0",
    "sass-loader": "^12.4.0",
    "sib-api-v3-sdk": "^8.2.1",
    "template.data.gouv.fr": "^1.3.2",
    "tmp": "^0.2.1",
    "validator": "^13.7.0",
    "vue": "^3.2.32",
    "vue-matomo": "^4.1.0",
    "vue-router": "^4.0.14",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "@types/iframe-resizer": "^3.5.9",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-plugin-typescript": "~4.5.15",
    "@vue/cli-plugin-unit-jest": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/vue3-jest": "^27.0.0",
    "argparse": "^2.0.1",
    "babel-jest": "^27.5.1",
    "cypress": "^9.2.1",
    "eslint": ">=7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-vue": "^8.0.3",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "memory-fs": "^0.5.0",
    "minimist": "^1.2.6",
    "mustache": "^4.2.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "ts-jest": "^28.0.5",
    "typescript": "~4.1.5",
    "webpack": "^5.64.3",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "babel": {
    "presets": [
      [
        "@vue/cli-plugin-babel/preset",
        {
          "useBuiltIns": "entry"
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "strict",
    "semi": false
  },
  "browserslist": [
    "> 0.1%, last 2 versions, Firefox ESR, not dead"
  ],
  "jest": {
    "roots": [
      "<rootDir>/tests"
    ],
    "moduleDirectories": [
      "node_modules",
      "."
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "@vue/vue3-jest",
      ".+\\.(css|styl|less|scss|sass|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "\\.[jt]sx?$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "transformIgnorePatterns": [
      "/\\./"
    ],
    "moduleNameMapper": {
      "(.+)\\.js": "$1",
      "@root/(.*)": "<rootDir>/$1",
      "^lodash-es$": "lodash"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/(integration|unit)/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "preset": "ts-jest",
    "automock": false,
    "setupFiles": [
      "./tests/unit/mocks/config.js"
    ]
  },
  "engines": {
    "node": ">= 16"
  },
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/betagouv/aides-jeunes"
  }
}
